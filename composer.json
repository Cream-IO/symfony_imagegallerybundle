{
  "type": "symfony-bundle",
  "license": "CC-BY-NC-SA-4.0",
  "name": "creamio/symfony_imagegallerybundle",
  "description": "Simple library to handle image galleries for CreamIO projects in symfony",
  "authors": [
    {
      "name": "QRaimbault"
    }
  ],
  "keywords": [],
  "require": {
    "php": "^7.2",
    "ext-iconv": "*",
    "gbprod/uuid-normalizer": "^1.1",
    "ramsey/uuid-doctrine": "^1.4",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/console": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/orm-pack": "^1.0",
    "symfony/validator": "^4.0",
    "creamio/symfony_basebundle": "^0",
    "creamio/symfony_uploadbundle": "^0"
  },
  "require-dev": {
    "behat/behat": "^3.4",
    "friendsofphp/php-cs-fixer": "^2.11",
    "behat/mink": "^1.7.1@dev",
    "behat/mink-browserkit-driver": "@dev",
    "behat/mink-extension": "^2.3",
    "behat/symfony2-extension": "^2.1",
    "symfony/maker-bundle": "^1.4",
    "behatch/contexts": "^3.1",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "emuse/behat-html-formatter": "^0.1.0",
    "phpunit/php-code-coverage": "^6.0",
    "rdx/behat-variables": "^1.2",
    "symfony/browser-kit": "~4.0",
    "symfony/dom-crawler": "~4.0",
    "symfony/dotenv": "^4.0",
    "symfony/phpunit-bridge": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "CreamIO\\UploadBundle\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
